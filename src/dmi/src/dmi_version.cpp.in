#include "dmi_version.hpp"


std::string getDmiVersion()
{
  return "@kbcpp-dmi_VERSION@";
}

unsigned getDmiVersionMajor()
{
  return @kbcpp-dmi_VERSION_MAJOR@;
}

unsigned getDmiVersionMinor()
{
  return @kbcpp-dmi_VERSION_MINOR@;
}

unsigned getDmiVersionPatch()
{
  return @kbcpp-dmi_VERSION_PATCH@;
}

